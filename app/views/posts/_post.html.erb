<turbo-frame id="<%= dom_id(post) %>">
  <li>
    <%= link_to post.title, post_path(post) %> - <%= post.user.email %>
    <%= link_to 'Edit', edit_post_path(post) %> |
    <%= link_to 'Delete', destroy_post_path(post), method: :delete, data: { turbo_confirm: 'Are you sure?' } %>
  </li>
</turbo-frame>
